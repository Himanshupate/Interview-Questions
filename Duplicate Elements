#include <iostream>
#include<stdio.h>

using namespace std;

int main() {
	int i,j,x,m,y;
	scanf("%d",&m);
	int a[m];
	for(i=0 ;i<m ; a[i] =   scanf("%d",&x),a[i] = x, i++){}
    y =m;
    while(y){
    	for(i = 0; i<m;i++){
    	    for(j = i+1; j<m;j++){
    	        if(a[j] == a[i]){
    	            a[j]++;
    	        }
    	    }
    	}
    	y--;
    }
	for(i=0 ;i<m ;printf("%d ", a[i]), i++){}
}
